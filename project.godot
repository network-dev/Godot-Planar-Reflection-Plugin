[autoload]

ReflectMaterialManager="*res://addons/Silicon.vfx.planar_reflection/reflect_material_manager.gd"

[dotnet]

project/assembly_name="Planar Reflection Plugin"

[editor_plugins]

enabled=PackedStringArray("res://addons/Silicon.vfx.planar_reflection/plugin.cfg")

[filesystem]

import/blender/enabled=false

[rendering]

environment/defaults/default_environment="res://default_env.tres"
quality/filters/msaa=2
